{
  "name": "ai-vector-db-clients-elasticsearch",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ai/vector-db-clients/elasticsearch/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/ai/vector-db-clients/elasticsearch",
        "main": "libs/ai/vector-db-clients/elasticsearch/src/index.ts",
        "tsConfig": "libs/ai/vector-db-clients/elasticsearch/tsconfig.lib.json",
        "assets": ["libs/ai/vector-db-clients/elasticsearch/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/ai/vector-db-clients/elasticsearch/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ai/vector-db-clients/elasticsearch/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": ["type:library", "scope:ai", "domain:vector-db"]
}
