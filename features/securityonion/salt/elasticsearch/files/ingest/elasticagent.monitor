{
  "processors": [
    {
      "set": {
        "field": "event.dataset",
        "value": "gridmetrics.agents",
        "ignore_failure": true
      }
    },
    {
      "set": {
        "field": "event.module",
        "value": "gridmetrics",
        "ignore_failure": true
      }
    },
    {
      "remove": {
        "field": [
          "host",
          "elastic_agent",
          "agent"
        ],
        "ignore_missing": true,
        "ignore_failure": true
      }
    },
    {
      "json": {
        "field": "message",
        "add_to_root": true,
        "ignore_failure": true
      }
    }
  ]
}