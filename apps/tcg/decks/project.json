{
  "name": "tcg-decks",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/tcg/decks/src",
  "projectType": "application",
  "tags": ["scope:tcg", "type:app"],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/tcg/decks"
      }
    },
    "dev": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "tcg-decks:build",
        "dev": true
      }
    },
    "start": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "tcg-decks:build",
        "dev": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
