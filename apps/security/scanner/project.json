{
  "name": "security-scanner",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/security/scanner/src",
  "projectType": "application",
  "tags": ["scope:security", "type:app"],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/security/scanner"
      }
    },
    "dev": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "security-scanner:build",
        "dev": true
      }
    },
    "start": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "security-scanner:build",
        "dev": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
